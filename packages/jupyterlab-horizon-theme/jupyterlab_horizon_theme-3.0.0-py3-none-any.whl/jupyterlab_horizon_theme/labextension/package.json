{
  "name": "@mohirio/jupyterlab-horizon-theme",
  "version": "3.0.0",
  "description": "VSCode Horizon Theme port for JupyterLab",
  "author": {
    "name": "Mohammad Almogbel",
    "email": "almogbelmohi@gmail.com"
  },
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "jupyterlab-theme"
  ],
  "license": "BSD-3-Clause",
  "files": [
    "lib/*.d.ts",
    "lib/*.js.map",
    "lib/*.js",
    "static/*.ttf",
    "static/index.css",
    "style/index.js"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "directories": {
    "lib": "lib/"
  },
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension:dev",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc -b",
    "build:prod": "jlpm run build:lib && jlpm run build:labextension",
    "build:webpack": "webpack",
    "clean": "jlpm run clean:lib",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension",
    "clean:labextension": "rimraf jupyterlab_horizon_theme/labextension",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "install:extension": "jupyter labextension develop --overwrite .",
    "prepare": "jlpm run clean && jlpm run build:prod",
    "watch": "run-p watch:src watch:labextension",
    "watch:labextension": "jupyter labextension watch .",
    "watch:src": "tsc -w",
    "watch:webpack": "webpack --watch"
  },
  "dependencies": {
    "@jupyterlab/application": "^3.0.6",
    "@jupyterlab/apputils": "^3.0.5"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "css-loader": "^5.1.2",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mini-css-extract-plugin": "~1.3.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "svg-url-loader": "^7.1.1",
    "svgo": "^2.2.2",
    "svgo-loader": "~3.0.0",
    "typedoc": "^0.20.31",
    "typescript": "~4.1.3",
    "url-loader": "~4.1.1",
    "watch": "~1.0.2",
    "webpack": "^5.25.0",
    "webpack-cli": "^4.5.0"
  },
  "sideEffects": [
    true,
    "style/index.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "themePath": "static/index.css",
    "outputDir": "jupyterlab_horizon_theme/labextension",
    "_build": {
      "load": "static/remoteEntry.a22ae48f695c1e0a015e.js",
      "extension": "./extension",
      "style": "./style"
    }
  },
  "homepage": "https://github.com/mohirio/jupyterlab-horizon-theme",
  "bugs": {
    "url": "https://github.com/mohirio/jupyterlab-horizon-theme/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mohirio/jupyterlab-horizon-theme.git"
  },
  "styleModule": "style/index.js"
}
