{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "simple": {
      "format": "%(levelname)s::%(name)s ** %(message)s"
    },
    "file": {
      "format": "%(asctime)s::%(levelname)s::%(name)s ** %(message)s",
      "datefmt": "%Y-%m-%d %H:%M:%S"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "INFO",
      "formatter": "simple",
      "stream": "ext://sys.stdout"
    },
    "debug_handler": {
      "class": "videotools.AppFileHandler",
      "level": "DEBUG",
      "formatter": "file",
      "when": "D",
      "interval": 1,
      "backupCount": 7,
      "filename": "debug.log",
      "encoding": "utf8"
    },
    "warn_handler": {
      "class": "videotools.AppFileHandler",
      "level": "WARNING",
      "formatter": "file",
      "when": "D",
      "interval": 1,
      "backupCount": 7,
      "filename": "warn.log",
      "encoding": "utf8"
    }
  },
  "loggers": {},
  "root": {
    "level": "INFO",
    "handlers": [
      "console",
      "debug_handler",
      "warn_handler"
    ]
  }
}
