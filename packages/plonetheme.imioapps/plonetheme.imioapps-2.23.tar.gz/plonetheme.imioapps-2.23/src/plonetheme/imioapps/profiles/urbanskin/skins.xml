<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="urbanskin">

 <object name="urbanskin_images"
    meta_type="Filesystem Directory View"
    directory="plonetheme.imioapps:skins/urbanskin_images"/>
 <object name="urbanskin_styles"
    meta_type="Filesystem Directory View"
    directory="plonetheme.imioapps:skins/urbanskin_styles"/>

 <skin-path name="urbanskin" based-on="Sunburst Theme">
   <layer name="imioapps_images"
          insert-after="custom"/>
   <layer name="imioapps_styles"
          insert-after="imioapps_images"/>
   <layer name="urbanskin_images"
          insert-after="custom"/>
   <layer name="urbanskin_styles"
          insert-after="urbanskin_images"/>
 </skin-path>

</object>
