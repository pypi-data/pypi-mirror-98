<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="pstskin">

 <object name="pstskin_images"
    meta_type="Filesystem Directory View"
    directory="plonetheme.imioapps:skins/pstskin_images"/>
 <object name="pstskin_styles"
    meta_type="Filesystem Directory View"
    directory="plonetheme.imioapps:skins/pstskin_styles"/>

 <skin-path name="pstskin" based-on="Sunburst Theme">
   <layer name="pstskin_images"
          insert-after="custom"/>
   <layer name="pstskin_styles"
          insert-after="pstskin_images"/>
   <layer name="imioapps_images"
          insert-after="custom"/>
   <layer name="imioapps_styles"
          insert-after="imioapps_images"/>
 </skin-path>

</object>
