<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="Sunburst Theme">

 <!-- imioapps related elements -->
 <object name="imioapps_images"
         remove="True"/>
 <object name="imioapps_styles"
         remove="True"/>

 <skin-path name="imioapps" based-on="Sunburst Theme" remove="True">
   <layer name="imioapps_images"
          remove="True"/>
   <layer name="imioapps_styles"
          remove="True"/>
 </skin-path>

 <!-- plonemeetingskin related elements -->
 <object name="plonemeetingskin_images"
         remove="True"/>
 <object name="plonemeetingskin_styles"
         remove="True"/>

