<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="dmsmailskin">

 <object name="dmsmailskin_images"
    meta_type="Filesystem Directory View"
    directory="plonetheme.imioapps:skins/dmsmailskin_images"/>
 <object name="dmsmailskin_styles"
    meta_type="Filesystem Directory View"
    directory="plonetheme.imioapps:skins/dmsmailskin_styles"/>

 <skin-path name="dmsmailskin" based-on="Sunburst Theme">
   <layer name="dmsmailskin_images"
          insert-after="custom"/>
   <layer name="dmsmailskin_styles"
          insert-after="dmsmailskin_images"/>
   <layer name="imioapps_images"
          insert-after="dmsmailskin_styles"/>
   <layer name="imioapps_styles"
          insert-after="imioapps_images"/>
 </skin-path>

</object>
