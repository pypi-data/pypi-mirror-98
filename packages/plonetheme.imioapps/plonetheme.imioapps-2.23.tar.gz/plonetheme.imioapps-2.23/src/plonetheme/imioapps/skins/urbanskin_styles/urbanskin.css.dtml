/*
** urban style sheet
*/

/* <dtml-with base_properties> (do not remove this :) */
/* <dtml-call "REQUEST.set('portal_url', portal_url())"> (not this either :) */

input[type=button], input[type=submit], button.context {
     background-position: 3px 3px;
     box-shadow: 1px 1px 3px grey;
}

#content, #portal-columns, div.portletAssignments {
line-height: 1.5em;
}

/* Colors on states */
.state-in_progress {
color: #EA6619 !important;
}

.state-enabled {
color: green !important;
}

.state-closed {
color: grey !important;
}

.state-disabled {
color: red !important;
}

.state-authorized {
color: green !important;
}

.state-accepted {
color: green !important;
}

.state-acceptable {
color: green !important;
}

.state-inacceptable {
color: red !important;
}

.state-retired {
color: grey !important;
}

.state-incomplete {
color: black !important;
}

.state-refused {
color: red !important;
}

.icons-on #visual-portal-wrapper table.listing td {
    line-height: 1.4em;
}

.icons-on #visual-portal-wrapper table.listing td a {
    font-size: 105%;
}

/* Color manual parcels in cyan and outdated parcels in orange*/
table.listing tbody tr span.outdated_parcel {
    background: none repeat scroll 0 0 orange;
}

table.listing tbody tr span.manual_parcel {
    background: none repeat scroll 0 0 cyan;
}

