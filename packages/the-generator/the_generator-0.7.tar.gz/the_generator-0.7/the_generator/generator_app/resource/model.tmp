from django.db import models
from django.urls import reverse
from django_extensions.db.fields import AutoSlugField
# this template for package
class {{name}}(model.Model):
    {% for attribute in attributes %}
    {{attribute.name}} = {{attribute.type}}({% for value_item in attribute.type_value %} {{value_item.name}}={{value_item.value}}, {% endfor %})
    {% endfor %}
    created = models.DateField(auto_now_add=True, editable=False)

    updated = models.DateField(auto_now=True, editable=False)


    def get_absolute_url(self):
        return reverse('{{ name | lower}}_detail', args=(self.{{ identifier }},))

    def get_update_url(self):
        return reverse('{{ name | lower}}_update', args=(self.{{ identifier }},))

    def get_delete_url(self):
        return reverse('{{ name | lower}}_delete', args=(self.{{ identifier }},))

    def get_list_url(self):
        return reverse('{{ name | lower}}_list')

    def get_create_url(self):
        return reverse('{{ name | lower}}_create')

    def __str__(self):
        return self.{{ model_representation }}
