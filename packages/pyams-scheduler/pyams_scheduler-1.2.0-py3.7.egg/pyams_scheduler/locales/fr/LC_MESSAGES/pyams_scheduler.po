msgid "Base target URI"
msgstr "URI de base"

#: src/pyams_scheduler/interfaces/task/rest.py:34
msgid "Base URI, including protocol and hostname of remote service"
msgstr ""
"URI de base, incluant uniquement le protocole et le nom d'hôte, du service"

#: src/pyams_scheduler/interfaces/task/rest.py:37
msgid "REST service"
msgstr "Service HTTP"

#: src/pyams_scheduler/interfaces/task/rest.py:38
msgid "Method and relative URL of REST service"
msgstr "Méthode et URL relative du service HTTP"

#: src/pyams_scheduler/interfaces/task/rest.py:42
msgid "Service parameters"
msgstr "Paramètres du service"

#: src/pyams_scheduler/interfaces/task/rest.py:43
msgid "Enter service parameters, in JSON object format"
msgstr "Indiquer les paramètres éventuels du service, au format JSON"

#: src/pyams_scheduler/interfaces/task/rest.py:46
msgid "Verify SSL?"
msgstr "Vérification SSL ?"

#: src/pyams_scheduler/interfaces/task/rest.py:47
msgid "If 'no', SSL certificates will not be verified"
msgstr "Si 'non', les certificats SSL ne seront pas vérifiés"

#: src/pyams_scheduler/interfaces/task/rest.py:51
msgid "Connection timeout"
msgstr "Délai de réponse"

#: src/pyams_scheduler/interfaces/task/rest.py:52
msgid ""
"Connection timeout, in seconds; keep empty to use system's default, which is "
"also none by default"
msgstr "Délai de réponse, en secondes"

#: src/pyams_scheduler/interfaces/task/rest.py:57
msgid "Allow redirects?"
msgstr "Autoriser les redirections"

#: src/pyams_scheduler/interfaces/task/rest.py:58
msgid "If disabled, redirections will now be handled"
msgstr ""
"Si cette option est désactivées, les redirections éventuelles ne seront pas "
"prises en charge"

#: src/pyams_scheduler/interfaces/task/rest.py:62
#: src/pyams_scheduler/interfaces/task/ssh.py:86
msgid "OK status"
msgstr "Statuts OK"

#: src/pyams_scheduler/interfaces/task/rest.py:63
msgid "Comma-separated list of HTTP status which may be considered as success"
msgstr "Liste de codes HTTP considérés comme \"OK\", séparés par des virgules"

