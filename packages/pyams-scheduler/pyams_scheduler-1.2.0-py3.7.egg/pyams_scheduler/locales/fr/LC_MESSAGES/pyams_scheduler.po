msgstr "Jours du mois (de 1 à 31) de planification de la tâche"

#: src/pyams_scheduler/interfaces/__init__.py:202
msgid "Weeks"
msgstr "Semaines"

#: src/pyams_scheduler/interfaces/__init__.py:203
msgid "Year weeks (1-53) for which to schedule the job"
msgstr "Numéros de semaines (de 1 à 53) de planification de la tâche"

#: src/pyams_scheduler/interfaces/__init__.py:207
msgid "Week days"
msgstr "Jours de la semaine"

#: src/pyams_scheduler/interfaces/__init__.py:208
msgid "Week days (0-6, with 0 as monday) for which to schedule the job"
msgstr ""
"Jours de la semaine (de 0 à 6, 0 étant le dimanche) de planification de la "
"tâche"

#: src/pyams_scheduler/interfaces/__init__.py:213
msgid "Hours"
msgstr "Heures"

#: src/pyams_scheduler/interfaces/__init__.py:214
msgid "Hours (0-23) for which to schedule the job"
msgstr ""
"Heures de la journée (de 0 à 23) de planification de la tâche. ATTENTION : "
"ces heures doivent être indiquées en GMT !!!"

#: src/pyams_scheduler/interfaces/__init__.py:218
msgid "Minutes"
msgstr "Minutes"

#: src/pyams_scheduler/interfaces/__init__.py:219
msgid "Minutes (0-59) for which to schedule the job"
msgstr "Minutes (de 0 à 59) de planification de la tâche"

#: src/pyams_scheduler/interfaces/__init__.py:223
msgid "Seconds"
msgstr "Secondes"

#: src/pyams_scheduler/interfaces/__init__.py:224
msgid "Seconds (0-59) for which to schedule the job"
msgstr "Secondes (de 0 à 59) de planification de la tâche"

#: src/pyams_scheduler/interfaces/__init__.py:241
msgid "Execution date"
msgstr "Date d'exécution"

#: src/pyams_scheduler/interfaces/__init__.py:242
msgid "Date and time on which execution should start"
msgstr "Date et heure à laquelle la tâche doit être exécutée"

