<?xml version="1.0"?>
<object name="portal_workflow" meta_type="Plone Workflow Tool">
 <object name="esd-answer-workflow" meta_type="Workflow"/>
 <object name="esd-comment-workflow" meta_type="Workflow"/>
 <object name="esd-question-review-workflow" meta_type="Workflow"/>
 <object name="esd-file-workflow" meta_type="Workflow"/>
 <object name="esd-review-workflow" meta_type="Workflow"/>
 <object name="esd-reviewtool-folder-workflow" meta_type="Workflow"/>
 <object name="esd-conclusion-workflow" meta_type="Workflow"/>
 <bindings>
  <type type_id="Comment">
   <bound-workflow workflow_id="esd-comment-workflow"/>
  </type>
  <type type_id="CommentAnswer">
   <bound-workflow workflow_id="esd-answer-workflow"/>
  </type>
  <type type_id="Observation">
   <bound-workflow workflow_id="esd-review-workflow"/>
  </type>
  <type type_id="Conclusions">
   <bound-workflow workflow_id="esd-conclusion-workflow"/>
  </type>
  <type type_id="Question">
   <bound-workflow workflow_id="esd-question-review-workflow"/>
  </type>
  <type type_id="ReviewFolder">
   <bound-workflow workflow_id="esd-reviewtool-folder-workflow"/>
  </type>
  <type type_id="NECDFile">
   <bound-workflow workflow_id="esd-file-workflow"/>
  </type>
  <type type_id="Discussion Item">
   <bound-workflow workflow_id="one_state_workflow"/>
  </type>

 </bindings>
</object>
