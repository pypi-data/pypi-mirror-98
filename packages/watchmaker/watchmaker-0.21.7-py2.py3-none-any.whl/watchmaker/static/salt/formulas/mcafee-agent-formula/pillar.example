mcafee-agent:

  lookup:

    #############################################################
    #
    # OPTIONAL Windows parameters for the mcafee-agent-formula
    #
    #############################################################

    # `package` is the name of the package definition for the
    # mcafee agent.
    #package: mcafee-agent

    # `version` is the version of the agent to install. the version
    # must be available in the package repository.
    #version: ''


    #############################################################
    #
    # REQUIRED Linux parameters for the mcafee-agent-formula
    # Uncomment and set all required parameters or the formula
    # will fail
    #############################################################

    # URL to the install shell archive
    #source: https://S3BUCKET.F.Q.D.N/mcafee/linux/mcafee-agent/dev/install.sh

    # URL to the hash of the shell archive
    #source_hash: https://S3BUCKET.F.Q.D.N/mcafee/linux/mcafee-agent/dev/install.sh.SHA512

    #############################################################
    #
    # OPTIONAL Linux parameters for the mcafee-agent-formula
    #
    #############################################################

    # Directory where McAfee SSL keyfiles are stored
    #keystore_directory: /opt/McAfee/cma/scratch/keystore

    # List of keyfiles to look for
    #key_files:
    #  - agentprvkey.bin
    #  - agentpubkey.bin
    #  - serverpubkey.bin
    #  - serverreqseckey.bin

    # List of RPMs to look for
    #rpms:
    #  - MFEcma
    #  - MFErt

    # List of ports to open inbound for remote-management
    #client_in_ports:
    #  - 8591

    # Options to pass to installer script
    # installer_opts: '-i -r'
