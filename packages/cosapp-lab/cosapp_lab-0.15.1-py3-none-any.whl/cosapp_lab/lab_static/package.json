{
  "name": "cosapp_lab",
  "version": "0.15.1",
  "description": "Toolbox for managing and deploying CoSApp powered dashboards.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "widgets"
  ],
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "homepage": "https://gitlab.com/cosapp/cosapp_lab",
  "bugs": {
    "url": "https://gitlab.com/cosapp/cosapp_lab/-/issues"
  },
  "license": "Proprietary License",
  "author": {
    "name": "CoSApp Team",
    "email": "duc-trung.le@safrangroup.com"
  },
  "scripts": {
    "build": "tsc  && webpack --env MODE=prod && jlpm run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:app": "tsc  && webpack --env TARGET=app",
    "build:lab": "tsc  && webpack --env TARGET=lab && jlpm build:labextension:dev",
    "build:all": "tsc  && webpack && jlpm build:labextension:dev",
    "build:webpack": "webpack --env TARGET=lab && jlpm build:labextension:dev",
    "clean:all": "rimraf buildjs/* && rimraf lib/* && rimraf cosapp_lab/lab_static/*  && rimraf cosapp_lab/app_static/static/dist/* &&  copyfiles ./package.json ./buildjs",
    "clean:lib": "rimraf buildjs/* && rimraf lib/* && rimraf cosapp_lab/lab_static/*",
    "clean:labextension": "rimraf cosapp_lab/lab_static/static/lib_index_* && rimraf cosapp_lab/lab_static/static/remoteEntry*",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "install:extension": "jupyter labextension develop --overwrite .",
    "copy:extension": "python ./cosapp_lab/utils/copy_extension.py",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "tsc -w",
    "watch:lab": "jupyter labextension watch .",
    "test": "jest src --silent "
  },
  "dependencies": {
    "@antv/g6": "^3.5.7",
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@jupyter-widgets/base": "^4.0.0",
    "@jupyter-widgets/controls": "^3.0.0",
    "@jupyter-widgets/html-manager": "^0.20.0",
    "@jupyterlab/application": "^3.0.0",
    "@jupyterlab/apputils": "^3.0.0",
    "@jupyterlab/coreutils": "^5.0.0",
    "@jupyterlab/notebook": "^3.0.0",
    "@jupyterlab/services": "^6.0.0",
    "@lumino/widgets": "^1.3.0",
    "@material-ui/core": "^4.9.3",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.33",
    "@projectstorm/react-diagrams": "^6.2.0",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "closest": "^0.0.1",
    "codemirror": "^5.59.4",
    "dagre": "^0.8.4",
    "flexlayout-react": "^0.5.4",
    "global": "^4.4.0",
    "ify-loader": "^1.1.0",
    "lodash": "^4.17.15",
    "material-table": "1.62.0",
    "mathjs": "^6.2.5",
    "ml-matrix": "^6.5.0",
    "pathfinding": "^0.4.18",
    "paths-js": "^0.4.10",
    "plotly.js": "^1.57.1",
    "react-codemirror2": "^7.2.1",
    "react-dnd": "^7.3.0",
    "react-dnd-html5-backend": "^7.4.4",
    "react-draggable": "^4.4.3",
    "react-plotly.js": "^2.5.0",
    "react-redux": "^7.1.3",
    "react-scripts": "3.2.0",
    "react-select": "^3.0.8",
    "react-sortable-tree": "2.7.1",
    "react-table": "^7.6.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "resize-observer-polyfill": "^1.5.1",
    "three": "^0.117.0"
  },
  "resolutions": {
    "lodash": "4.17.15",
    "react-sortable-tree": "2.7.1"
  },
  "devDependencies": {
    "@babel/core": "7",
    "@babel/preset-env": "7",
    "@jupyterlab/builder": "^3.0.0",
    "@testing-library/react": "^9.4.1",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "24",
    "@types/node": "^10.11.6",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-sortable-tree": "^0.3.6",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "copyfiles": "^2.4.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "fs-extra": "^7.0.0",
    "jest": "24",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.0",
    "prettier-eslint": "^12.0.0",
    "re-resizable": "^6.3.2",
    "redux-mock-store": "^1.5.4",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.4",
    "ts-jest": "24",
    "ts-loader": "^5.2.1",
    "typescript": "^3.8.3",
    "webpack-cli": "^4.5.0"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true,
    "outputDir": "cosapp_lab/lab_static",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true
      },
      "react": {
        "bundled": false,
        "singleton": true
      },
      "codemirror": {
        "bundled": false,
        "singleton": true
      }
    },
    "discovery": {
      "server": {
        "base": {
          "name": "cosapp_lab"
        },
        "managers": [
          "conda"
        ]
      }
    },
    "_build": {
      "load": "static/remoteEntry.47e634463d9289749475.js",
      "extension": "./extension"
    }
  }
}
