include HISTORY.rst
include LICENSE
include README.md

include jupyter-config/cosapp_lab.json
include jupyter-config/cosapp_lab_server.json
include package.json
include tsconfig.json

include pyproject.toml
include setupbase.py
recursive-include cosapp_lab/* *.ipynb
recursive-include cosapp_lab/utils/templates/cookiecutter-cosapp-workspace/{{cookiecutter.project_slug}} *

graft cosapp_lab/utils
graft cosapp_lab/module/templates
graft cosapp_lab/lab_static
graft cosapp_lab/app_static


# Javascript files
graft src
graft style
prune **/node_modules
prune coverage
prune lib

# Patterns to exclude from any directory
global-exclude *~
global-exclude *.pyc
global-exclude *.pyc*
global-exclude *.pyo
global-exclude .git
global-exclude .ipynb_checkpoints
