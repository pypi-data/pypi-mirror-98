{
  "name": "jupyterlab_optumi",
  "version": "3.5.1",
  "description": "Offload Jupyter notebooks from your laptop into the cloud",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://optumi.com",
  "author": {
    "name": "Optumi Inc Authors",
    "email": "cs@optumi.com"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "style/index.js"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.js",
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension:dev",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "build:prod": "jlpm run build:lib && jlpm run build:labextension",
    "clean": "jlpm run clean:lib",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension",
    "clean:labextension": "rimraf jupyterlab_optumi/labextension",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "install:extension": "jupyter labextension develop --overwrite .",
    "prepare": "jlpm run clean && jlpm run build:prod",
    "watch": "run-p watch:src watch:labextension",
    "watch:labextension": "jupyter labextension watch .",
    "watch:src": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^3.0.4",
    "@jupyterlab/apputils": "^3.0.3",
    "@jupyterlab/coreutils": "^5.0.2",
    "@jupyterlab/notebook": "^3.0.4",
    "@lumino/coreutils": "^1.5.3",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@stripe/stripe-js": "^1.9.0",
    "@types/codemirror": "^0.0.106",
    "@types/node": "^13.13.12",
    "@types/react-linkify": "^1.0.0",
    "ansi-to-react": "^6.0.8",
    "moment": "^2.29.1",
    "notistack": "^1.0.1",
    "react": "^17.0.1",
    "react-bootstrap": "^1.3.0",
    "react-codemirror2": "^7.2.1",
    "react-dom": "^17.0.1",
    "react-linkify": "^1.0.0-alpha",
    "react-markdown": "^5.0.3",
    "remark-gfm": "^1.0.0"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mkdirp": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "typescript": "~4.1.3"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "jupyterlab": {
    "extension": true,
    "outputDir": "jupyterlab_optumi/labextension"
  },
  "styleModule": "style/index.js"
}
